{
  "name": "gikopoi2",
  "version": "0.0.1",
  "description": "gikopoi2",
  "scripts": {
    "build": "npx tsc",
    "deploy-prod": "vue-cli-service build && git add dist && git commit -a -m vue.jsbuild && git push origin master:CI-PROD",
    "deploy-test": "vue-cli-service build && git add dist && git commit -a -m vue.jsbuild && git push origin HEAD:CI-TEST -f",
    "dev-remote-persistor": "cross-env PERSISTOR_URL=http://localhost:8086 PERSISTOR_SECRET=hello ts-node-dev --respawn index.ts",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "serve-be": "ts-node-dev --respawn src/backend/index.ts",
    "serve-fe": "vue-cli-service serve",
    "start": "node backend-build/src/backend/index.js",
    "start-persistor": "cross-env PERSISTOR_SECRET=hello node state-persistor.js"
  },
  "main": "backend-build/src/backend/index.js",
  "dependencies": {
    "chess.js": "^0.12.0",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.16.2",
    "express-sslify": "^1.2.0",
    "got": "^11.8.1",
    "janus-videoroom-client": "^4.1.10",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "lodash": "^4.17.20",
    "loglevel": "^1.7.1",
    "socket.io": "^4.0.2",
    "socket.io-client": "^4.1.3",
    "tripcode": "file:./src/backend/tripcode",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "vue": "^3.1.5",
    "vue-i18n": "^9.1.0",
    "webrtc-codec-support": "^1.0.1"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^2.1.0",
    "@types/chess.js": "^0.11.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.9",
    "@types/got": "^9.6.11",
    "@types/jquery": "^3.5.6",
    "@types/jqueryui": "^1.12.16",
    "@types/lodash": "^4.14.168",
    "@types/node": "^16.3.1",
    "@types/uuid": "^8.3.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "jquery-ui-bundle": "^1.12.1-migrate",
    "nodemon": "^2.0.6",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "~4.3.5",
    "vue-cli-plugin-i18n": "~2.1.2"
  },
  "engines": {
    "node": "14.17.0"
  },
  "license": "Beerware"
}
